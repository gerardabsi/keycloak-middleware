{
  "env": {
    "node": true,
    "mocha": true
  },
  "extends": "airbnb-base",
  "rules": {
    "max-len": [
      "error",
      {
        "code": 140
      }
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "no-underscore-dangle": "error",
    "no-console": "error",
    "no-multi-spaces": "error",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  }
}